    <%= flash[:message] if flash[:message] %>
    <%= form_for(@profile) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %><br>
      <%= f.label :address %>
      <%= f.text_field :address %><br>
      <%= f.label :phone %>
      <%= f.number_field :phone %><br>
      <%= f.submit %>
    <% end %>
