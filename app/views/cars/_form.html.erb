<h1>Please add some information about your vehicle:</h1><br />
<%= render 'shared/errors', model: @car %>
<%= form_for([@user, @car]) do |f| %>
  <%= f.label :make %>
  <%= f.text_field :make %><br>
  <%= f.label :model %>
  <%= f.text_field :model %><br>
  <%= f.label :year %>
  <%= f.number_field :year %><br>
  <%= f.label :color %>
  <%= f.text_field :color %><br>
  <%= f.submit class: "waves-effect waves-light btn" %><br><br><br>
<% end %>
